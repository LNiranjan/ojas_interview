<p id="notice"><%= notice %></p>


<%= render 'Search', tv_show: @tv_show %>
<h1>Tv Shows</h1>

<table>
  <thead>
    <tr>
      <th>Channel</th>
      <th>Show</th>
      <th>Timing</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tv_shows.each do |tv_show| %>
      <tr>
        <td><%= tv_show.channel %></td>
        <td><%= tv_show.show %></td>
        <td><%= tv_show.timing %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tv Show', new_tv_show_path %>
